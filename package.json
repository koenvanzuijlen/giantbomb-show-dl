    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "pinst": "3.0.0",
